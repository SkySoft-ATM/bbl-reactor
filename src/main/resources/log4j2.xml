<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="warn">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <!--  <PatternLayout pattern="%d{ISO8601} %-5p [%t] %c %X{traceId}: %m%n"/> -->
             <PatternLayout pattern="%d{ISO8601} %-5p [%t] %X{traceId}: %m%n"/>
         </Console>
     </Appenders>
     <Loggers>
         <Root level="info">
             <AppenderRef ref="Console"/>
         </Root>
        <!--  <Logger name="com.skysoftatm.bblreactor" level="debug"/> -->
    </Loggers>
</Configuration>
